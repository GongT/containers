{
	"git.detectSubmodules": false,
	"quicktask.enableShell": true,
	"files.associations": {
		"*.conflist": "json"
	},
	"files.watcherExclude": {
		"**/nginx/source/**": true,
		"**/wordpress/fs/project/**": true
	},
	"[javascript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[typescript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	}
}
